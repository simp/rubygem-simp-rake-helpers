#!/usr/bin/env ruby
# frozen_string_literal: true

require 'rubygems' unless defined?(Gem)
require 'beaker-docker'

VERSION_STRING = <<'VER'
                                 _ .--.
                                ( `    )
        beaker-docker         .-'      `--,
                  _..----.. (             )`-.
                .'_|` _|` _|(  .__,           )
               /_|  _|  _|  _(        (_,  .-'
              ;|  _|  _|  _|  '-'__,--'`--'
              | _|  _|  _|  _| |
          _   ||  _|  _|  _|  _| %s
        _( `--.\_|  _|  _|  _|/
     .-'       )--,|  _|  _|.`
    (__, (_      ) )_|  _| /
      `-.__.\ _,--'\|__|__/
                    ;____;
                     \YT/
                      ||
                     |""|
                     '=='
VER

puts VERSION_STRING % BeakerDocker::VERSION

exit 0
